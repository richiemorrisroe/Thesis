  title = {Effects of Etiology on Perceived Helpfulness of Treatments for Depression},
  journal = {Cognitive Therapy and Research},
  year = {2003},
  volume = {27},
  pages = {205-222},
  number = {2},
  __markedentry = {[Richard Morrisroe]},
  abstract = {Perceptions of the credibility and helpfulness of various treatments
	for depression can affect the success of different treatment options.
	While previous research has demonstrated individual differences in
	credibility ratings, no studies have evaluated the impact of different
	etiological theories on the perceptions of various treatment options.
	In this study, 36 mental health clients and 36 undergraduates rated
	7 depression treatments presented first alone, then with 6 different
	etiological vignettes. All participants considered treatments more
	helpful when cause and treatment focus were congruent (i.e., both
	psychological or both physical). Students regarded treatments as
	less helpful than did clients. We discuss implications for treatment
	credibility and acceptability research, and suggest ways of presenting
	to clients etiological information in relation to treatments.},
  keywords = {Behavioral Science},
