@ARTICLE{Kalka2009a,
  author = {Kalka, D. and Sobieszczanska, M. and Pilecki, W. and Bielous-Wilk,
	A. and Poreba, M. and Marciniak, W. and Glabek, K. and Turbanski,
	J. and Adamus, J.},
  title = {[Erectile dysfunction vs. physical capacity and physical activity
	at invasive treated patients with ischemic heart disease]},
  journal = {Pol Merkur Lekarski},
  year = {2009},
  volume = {27},
  pages = {284-9},
  number = {160},
  note = {Kalka, Dariusz Sobieszczanska, Malgorzata Pilecki, Witold Bielous-Wilk,
